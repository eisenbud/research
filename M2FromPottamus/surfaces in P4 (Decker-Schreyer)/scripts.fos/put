incr-set prlevel 1
if #0=1 START
incr-set prlevel -1
;;; Usage:
;;; 	<put matrix
;;;
;;;	putmat matrix >matrix
;;;
incr-set prlevel 1
jump END
;;; Parameters:
;;;
;;; Output values:
;;;
;;; (discussion)
;;;
;;; Caveats:
;;;
; created ...
START:
incr-set prcomment -1 
putmat #1 >#1
incr-set prcomment 1
END:
incr-set prlevel -1

